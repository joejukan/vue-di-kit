{
    "spec_dir": "spec",
    "spec_files": [
        "**/*[sS]pec.ts"
    ],
    "helpers": [
        "helpers/**/*.js"
    ],
    "reporters": [
        {
            "name": "jasmine-spec-reporter#SpecReporter",
            "options": {
                "displaySuccessful": true,
                "displayFailed": true,
                "displayPending": true,
                "displayDuration": true
            }
        }
    ]
}